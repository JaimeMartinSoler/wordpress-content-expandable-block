{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/content-expandable-block",
	"version": "0.1.0",
	"title": "Content Expandable Block",
	"category": "widgets",
	"description": "Content Expandable Block",
	"example": {},
	"attributes": {
        "buttonsTextColor": {
            "type": "string",
            "default": "#303030"
        },
        "buttonsColor": {
            "type": "string",
            "default": "#e0e0e0"
        },
        "buttonsFontSize": {
            "type": "string",
            "default": "22px"
        },
        "buttonsSize": {
            "type": "string",
            "default": "30px"
        },
        "buttonsHoverColor": {
            "type": "string",
            "default": "#ffffff"
        },
        "buttonsDisableColor": {
            "type": "string",
            "default": "#909090"
        },
        "buttonsBackgroundColor": {
            "type": "string",
            "default": "#aaaaaa"
        }
	},
	"supports": {
		"color": {
			"background": true,
			"text": true
		},
		"html": false,
		"typography": {
			"fontSize": true
		}
	},
	"textdomain": "content-expandable-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
